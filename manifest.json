{
  "name": "Learning English",
  "version": "1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "js": ["content-style.js", "content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
